<?xml version="1.0"?>
<Container version="2">
  <Name>Podfox</Name>
  <Repository>stevenmccurdy/podfox</Repository>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://gitlab.com/stevenmccurdy/podfox</Support>
  <Overview>A program for managing and catching podcasts from the terminal(and automatically).</Overview>
  <Beta>False</Beta>
  <Category>Downlaoder: Tools: MediaApp:Other</Category>
  <Date>2019-03-09</Date>
  <Icon></Icon>
  <ExtraParams/>
  <Description>A program for managing and catching podcasts from the terminal(and automatically).</Description>
  <Registry>https://cloud.docker.com/u/stevenmccurdy/repository/docker/stevenmccurdy/podfox</Registry>
  <GitHub>https://github.com/stevenmccurdy/unraid-docker-templates</GitHub>
  <Project>https://gitlab.com/stevenmccurdy/podfox</Project>

  <Config Name="Podcasts Path" Target="/podcasts" Default="/mnt/user/media/podcasts" Mode="rw" Description="Container path: /podcasts" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/media/podcasts</Config>

  <Config Name="Config Path" Target="/root" Default="/mnt/user/appdata/podfox" Mode="rw" Description="Container path: /root" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/podfox</Config>
</Container>
